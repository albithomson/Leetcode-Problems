def linearSearchearch(arr,target):
    for i in range(0,len(arr)):
        if arr[i]==target:
            return i
    return -1

arr=[5,4,-7,8,5]
target=8
result=linearSearchearch(arr,target)
if(result == -1):
    print("Element not found")
else:
    print("Element found at index: ", result) 
    
